<launch>

<!--カメラID clid, crid は arv-tool-0.4 から取得するのが正しい? --> 


<!--   実行例1(デフォルト値使用。)：-->
<!--    roslaunch camera.launch -->

<!--   実行例2(カメラID指定。左右を逆に。)：-->
<!--    roslaunch camera.launch clid:=SENTECH-17AB756 crid:=SENTECH-17AB755 -->

<!--
  <node name="camera" pkg="camera_aravis" type="camnode" args="SENTECH-17AB755">
    <param name="AcquisitionFrameRate" type="double" value="2.5" />
    <param name="ExposureTimeAbs" type="int" value="56666" />
  </node> 

  <node name="dynparam_cam" pkg="dynamic_reconfigure" type="dynparam" args="set /camera &quot;{'AcquisitionFrameRate':'4.6', 'ExposureTimeAbs':'23666'}&quot;" />

  <node name="dynparam_cam" pkg="dynamic_reconfigure" type="dynparam" args="load /camera $(find rovi)/camera_dump.yaml" />
  <node name="dynparam_cam1" pkg="dynamic_reconfigure" type="dynparam" args="load /rovi/left/camera $(find rovi)/left_dynparam_dump.yaml" />
  <node name="dynparam_cam2" pkg="dynamic_reconfigure" type="dynparam" args="load /rovi/right/camera $(find rovi)/right_dynparam_dump.yaml" />
-->



  <arg name="clid" default="SENTECH-17AB755" />
  <arg name="crid" default="SENTECH-17AB756" />

  <rosparam command="load" ns="/rovi" file="$(find rovi)/yaml/rovi_param.yaml" />

  <node ns="/rovi/left" name="camera" pkg="camera_aravis" type="camnode" args="$(arg clid)" />
  <node ns="/rovi/right" name="camera" pkg="camera_aravis" type="camnode" args="$(arg crid)" />



  <!-- The 'name' value of type='*.js' is required but not used. -->
  <!--   ros.initNode() argument in script/*.js specifies the actual name. -->
  <node ns="/rovi" name="tools_node" pkg="rovi" type="tools_node" />
  <node ns="/rovi" name="setgain" pkg="rovi" type="setgain.js" />
  <node ns="/rovi/left" name="swtrigger_l" pkg="rovi" type="swtrigger.js" args="--lr l" />
  <node ns="/rovi/right" name="swtrigger_r" pkg="rovi" type="swtrigger.js" args="--lr r" />
  <node ns="/rovi" name="livestream" pkg="rovi" type="livestream.js" />
  <node ns="/rovi" name="stillcapture" pkg="rovi" type="stillcapture.js" />
  <node ns="/rovi" name="parameter" pkg="rovi" type="parameter.js" />
  <node ns="/rovi" name="projector" pkg="rovi" type="projector.js" />
  <node ns="/rovi/left" name="remap_node" pkg="rovi" type="remap_node" />
  <node ns="/rovi/right" name="remap_node" pkg="rovi" type="remap_node" />
  <node ns="/rovi" name="genpc_node" pkg="rovi" type="genpc_node" />
  <node ns="/rovi" name="main_node" pkg="rovi" type="main_node.js" args="--mode test" />



</launch>


